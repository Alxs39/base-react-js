{
	"$schema": "https://turbo.build/schema.json",
	"ui": "tui",
	"tasks": {
		"build": {
			"dependsOn": ["^build"],
			"inputs": ["$TURBO_DEFAULT$", ".env*"],
			"outputs": ["dist/**"]
		},
		"lint": {
			"dependsOn": ["^lint"],
			"inputs": ["$TURBO_DEFAULT$", "eslint.config.js"]
		},
		"lint:fix": {
			"dependsOn": ["^lint:fix"],
			"inputs": ["$TURBO_DEFAULT$", "eslint.config.js"],
			"cache": false
		},
		"dev": {
			"cache": false,
			"persistent": true
		}
	}
}
