{
  "name": "base-react-js",
  "private": true,
  "type": "module",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "dev": "turbo dev",
    "build": "turbo build",
    "dev:native": "turbo -F native dev",
    "dev:web": "turbo -F web dev",
    "lint": "turbo run lint",
    "lint:fix": "turbo run lint:fix",
    "lint:root": "eslint . --ext js,jsx --report-unused-disable-directives --max-warnings 0",
    "lint:root:fix": "eslint . --ext js,jsx --fix"
  },
  "dependencies": {},
  "devDependencies": {
    "@eslint/js": "^9.38.0",
    "@stylistic/eslint-plugin": "^5.5.0",
    "eslint": "^9.38.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.0",
    "eslint-plugin-react-refresh": "^0.4.24",
    "turbo": "^2.5.4"
  },
  "packageManager": "pnpm@10.13.1"
}
